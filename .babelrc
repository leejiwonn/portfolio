{
  "presets": [
    "next/babel",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic",
        "importSource": "@emotion/react"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      },
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "~": "./src"
        }
      },
      "@emotion/babel-plugin"
    ]
  ]
}
